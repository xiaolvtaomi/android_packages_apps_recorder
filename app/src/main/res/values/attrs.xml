<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="visualizer_style">

        <attr name="visualizer_gravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>

        <attr name="visualizer_width" format="integer" />
        <attr name="visualizer_height" format="integer" />
    </declare-styleable>

</resources>
